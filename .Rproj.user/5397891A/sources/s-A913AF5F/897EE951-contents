library(R6)
aClass <- R6Class(
  classname = "aClass",
  inherit = NULL,
  portable = TRUE,

  private = list(.x = NULL),

  public = list(
    name = NULL,
    number=NULL,
    initialize = function(x) {
      self$name<-x
      private$.x <- x
      invisible(self)
    }
  )
)

bClass <- R6Class(
  classname = "aClass",
  inherit = aClass,
  portable = TRUE,

  private = list(.x = NULL),

  public = list(
    name = NULL,

    initialize = function(x) {
      self$name<-x
      private$.x <- x
      invisible(self)
    }
  )
)


a <- aClass$new("aname")
class(a)

b <- bClass$new("bname")
class(b)
b$number


